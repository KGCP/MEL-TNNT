			"flair_pooled": 1,
			"deeppavlov_onto": 1,
			"deeppavlov_onto_bert": 1,
			"deeppavlov_conll2003": 1,
			"deeppavlov_conll2003_bert": 1,
			"allennlp": 1,
			"allennlp_finegrained": 1,
			"polyglot": 1
		}
	},
